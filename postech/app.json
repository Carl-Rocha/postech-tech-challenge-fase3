{
  "expo": {
    "name": "Byte Bank",
    "slug": "byte-bank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bytebank",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.carlos.rocha.postech"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "firebase": {
        "apiKey": "AIzaSyAP1v_XqT3gwRxl3chsLvAYkXvPG5gsO3Y",
        "authDomain": "postech-fase-3.firebaseapp.com",
        "projectId": "postech-fase-3",
        "storageBucket": "postech-fase-3.firebasestorage.app",
        "messagingSenderId": "835966693874",
        "appId": "1:835966693874:web:182adf7fb2574f45613b6b"
      },
      "eas": {
        "projectId": "6a58d9be-aeb5-4d2a-b614-e47a6e8bc04c"
      }
    }
  }
}
